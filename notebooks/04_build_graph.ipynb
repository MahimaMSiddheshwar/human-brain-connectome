 "cells": [
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Notebook 04: Build Connectome Graph\n",
    "\n",
    "Build a weighted undirected network graph from functional connectivity data."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Import all required libraries\n",
    "import numpy as np\n",
    "import networkx as nx\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "import json\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "print(\"✓ All required libraries imported successfully\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Set random seed for reproducibility\n",
    "np.random.seed(42)\n",
    "\n",
    "print(\"✓ Random seed set to 42\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define paths\n",
    "notebook_dir = Path.cwd()\n",
    "project_dir = notebook_dir.parent\n",
    "data_dir = project_dir / 'data'\n",
    "figures_dir = project_dir / 'figures'\n",
    "results_dir = project_dir / 'results'\n",
    "\n",
    "# Load config\n",
    "config_path = data_dir / 'config.json'\n",
    "with open(config_path, 'r') as f:\n",
    "    config = json.load(f)\n",
    "\n",
    "print(f\"✓ Configuration loaded\")"
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],
   "source": [
    "# Load functional_matrix.npy\n",
    "print(\"Loading functional connectivity matrix...\")\n",
    "functional_matrix_path = data_dir / 'functional_matrix.npy'\n",
    "correlation_matrix = np.load(functional_matrix_path)\n",
    "\n",
    "print(f\"✓ Functional connectivity matrix loaded\")\n",
    "print(f\"  Matrix shape: {correlation_matrix.shape}\")\n",
    "print(f\"  Value range: [{correlation_matrix.min():.4f}, {correlation_matrix.max():.4f}]\")"
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],\n",
    "source": [
    "# Apply threshold of 0.3 (keep correlations above 0.3)\n",
    "print(\"\\nApplying correlation threshold...\")\n",
    "threshold = 0.3\n",
    "thresholded_matrix = correlation_matrix.copy()\n",
    "thresholded_matrix[np.abs(thresholded_matrix) < threshold] = 0\n",
    "\n",
    "n_connections_before = np.count_nonzero(np.triu(correlation_matrix, k=1))\n",
    "n_connections_after = np.count_nonzero(np.triu(thresholded_matrix, k=1))\n",
    "\n",
    "print(f\"✓ Threshold applied: r > {threshold}\")\n",
    "print(f\"  Connections before: {n_connections_before}\")\n",
    "print(f\"  Connections after: {n_connections_after}\")\n",
    "print(f\"  Retained: {100*n_connections_after/n_connections_before:.1f}%\")"
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],
   "source": [
    "# Build undirected weighted NetworkX Graph\n",
    "print(\"\\nBuilding NetworkX graph...\")\n",
    "G = nx.Graph()\n",
    "\n",
    "# Get atlas region labels (163 regions for Destrieux)\n",
    "n_regions = thresholded_matrix.shape[0]\n",
    "\n",
    "# Add nodes with attributes\n",
    "for i in range(n_regions):\n",
    "    G.add_node(i, region_index=i, region_name=f'Region_{i+1}')\n",
    "\n",
    "# Add edges with attributes\n",
    "for i in range(n_regions):\n",
    "    for j in range(i+1, n_regions):\n",
    "        weight = thresholded_matrix[i, j]\n",
    "        if weight != 0:  # Only add edges with non-zero weight\n",
    "            G.add_edge(i, j, weight=weight, abs_weight=abs(weight))\n",
    "\n",
    "print(f\"✓ Graph built successfully\")\n",
    "print(f\"  Number of nodes: {G.number_of_nodes()}\")\n",
    "print(f\"  Number of edges: {G.number_of_edges()}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],\n",
    "source": [
    "# Print graph properties\n",
    "print(\"\\nGraph Properties:\")\n",
    "print(\"-\" * 50)\n",
    "\n",
    "# Calculate density\n",
    "density = nx.density(G)\n",
    "print(f\"  Density: {density:.4f}\")\n",
    "\n",
    "# Calculate average degree\n",
    "degrees = [d for n, d in G.degree()]\n",
    "avg_degree = np.mean(degrees)\n",
    "print(f\"  Average degree: {avg_degree:.2f}\")\n",
    "print(f\"  Min degree: {min(degrees)}\")\n",
    "print(f\"  Max degree: {max(degrees)}\")\n",
    "\n",
    "# Check connectivity\n",
    "is_connected = nx.is_connected(G)\n",
    "n_components = nx.number_connected_components(G)\n",
    "print(f\"  Connected: {is_connected}\")\n",
    "print(f\"  Number of connected components: {n_components}\")\n",
    "\n",
    "print(\"-\" * 50)"
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],\n",
    "source": [
    "# Save graph as results/human_connectome.graphml\n",
    "print(\"\\nSaving graph...\")\n",
    "graphml_path = results_dir / 'human_connectome.graphml'\n",
    "\n",
    "nx.write_graphml(G, str(graphml_path))\n",
    "\n",
    "print(f\"✓ Graph saved to {graphml_path}\")\n",
    "print(f\"  File size: {graphml_path.stat().st_size / (1024**2):.2f} MB\")"
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],\n",
    "source": [
    "# Set matplotlib style\n",
    "plt.rcParams['font.family'] = 'DejaVu Sans'\n",
    "plt.rcParams['font.size'] = 8\n",
    "plt.rcParams['savefig.dpi'] = 300\n",
    "plt.rcParams['savefig.bbox'] = 'tight'\n",
    "\n",
    "print(\"✓ Matplotlib rcParams configured\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Plot degree distribution histogram\n",
    "print(\"\\nGenerating degree distribution plot...\")\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(8, 6))\n",
    "\n",
    "degrees_list = [G.degree(n, weight='weight') for n in G.nodes()]\n",
    "ax.hist(degrees_list, bins=20, color='steelblue', edgecolor='black', alpha=0.7)\n",
    "\n",
    "ax.set_xlabel('Node Degree (Weighted)', fontsize=9)\n",
    "ax.set_ylabel('Number of Nodes', fontsize=9)\n",
    "ax.set_title('Degree Distribution of Brain Connectome Network', fontsize=10, fontweight='bold')\n",
    "ax.grid(True, alpha=0.3, axis='y')\n",
    "\n",
    "# Add statistics\n",
    "stats_text = f'Mean: {np.mean(degrees_list):.2f}\\nStd: {np.std(degrees_list):.2f}'\n",
    "ax.text(0.98, 0.97, stats_text, transform=ax.transAxes, \n",
    "        verticalalignment='top', horizontalalignment='right',\n",
    "        bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.5),\n",
    "        fontsize=8)\n",
    "\n",
    "plt.tight_layout()\n",
    "\n",
    "# Save as PDF and PNG\n",
    "pdf_path = figures_dir / 'degree_distribution.pdf'\n",
    "png_path = figures_dir / 'degree_distribution.png'\n",
    "\n",
    "plt.savefig(pdf_path, dpi=300, bbox_inches='tight')\n",
    "plt.savefig(png_path, dpi=150, bbox_inches='tight')\n",
    "plt.close()\n",
    "\n",
    "print(f\"✓ Degree distribution plot saved:\")\n",
    "print(f\"  PDF: {pdf_path}\")\n",
    "print(f\"  PNG: {png_path}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Print completion summary\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"GRAPH CONSTRUCTION SUMMARY\")\n",
    "print(\"=\"*60)\n",
    "print(f\"\\nGraph Properties:\")\n",
    "print(f\"  Nodes: {G.number_of_nodes()}\")\n",
    "print(f\"  Edges: {G.number_of_edges()}\")\n",
    "print(f\"  Density: {density:.4f}\")\n",
    "print(f\"  Connected components: {n_components}\")\n",
    "print(f\"\\nFiles saved:\")\n",
    "print(f\"  Graph (GraphML): {graphml_path}\")\n",
    "print(f\"  Degree distribution: {pdf_path}\")\n",
    "print(f\"\\nNext Step: Run Notebook 05 (Graph Analysis)\")\n",
    "print(\"=\"*60)"
   ]
  }
