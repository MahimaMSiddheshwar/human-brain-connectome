   "source": [
    "# Notebook 06: Visualization\n",
    "\n",
    "Create comprehensive publication-quality visualizations of the connectome."
   ]
  },
  {
   "cell_type": {"metadata": {},
   "execution_count": null,
   "outputs": [],
   "source": [
    "# Import all required libraries\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import networkx as nx\n",
    "from pathlib import Path\n",
    "import json\n",
    "from nilearn import plotting\n",
    "from nilearn.datasets import fetch_atlas_destrieux_2009\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "print(\"✓ All required libraries imported successfully\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Set random seed for reproducibility\n",
    "np.random.seed(42)\n",
    "\n",
    "print(\"✓ Random seed set to 42\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Set matplotlib style\n",
    "plt.rcParams['font.family'] = 'DejaVu Sans'\n",
    "plt.rcParams['font.size'] = 8\n",
    "plt.rcParams['savefig.dpi'] = 300\n",
    "plt.rcParams['savefig.bbox'] = 'tight'\n",
    "\n",
    "print(\"✓ Matplotlib rcParams configured\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Define paths\n",
    "notebook_dir = Path.cwd()\n",
    "project_dir = notebook_dir.parent\n",
    "data_dir = project_dir / 'data'\n",
    "figures_dir = project_dir / 'figures'\n",
    "results_dir = project_dir / 'results'\n",
    "\n",
    "# Load config\n",
    "config_path = data_dir / 'config.json'\n",
    "with open(config_path, 'r') as f:\n",
    "    config = json.load(f)\n",
    "\n",
    "print(f\"✓ Configuration loaded\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execute_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Load all required data\n",
    "print(\"Loading data for visualization...\")\n",
    "\n",
    "# Load functional connectivity matrix\n",
    "connectivity_matrix = np.load(data_dir / 'functional_matrix.npy')\n",
    "\n",
    "# Load graph\n",
    "G = nx.read_graphml(str(results_dir / 'human_connectome.graphml'))\n",
    "G = nx.convert_node_labels_to_integers(G)\n",
    "\n",
    "# Load metrics\n",
    "metrics_df = pd.read_csv(results_dir / 'hub_regions.csv')\n",
    "\n",
    "# Load communities\n",
    "with open(results_dir / 'communities.json', 'r') as f:\n",
    "    communities_data = json.load(f)\n",
    "\n",
    "print(f\"✓ All data loaded successfully\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# Create community mapping for visualization\n",
    "node_to_community = {}\n",
    "for node_str, comm_id in communities_data['node_community_mapping'].items():\n",
    "    node_to_community[int(node_str)] = comm_id\n",
    "\n",
    "print(f\"✓ Community mapping created\")\n",
    "print(f\"  Communities: {len(communities_data['communities'])}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# FIGURE 1: Connectivity heatmap (already created in Notebook 03)\n",
    "print(\"\\nFigure 1: Connectivity heatmap (already saved from Notebook 03)\")\n",
    "print(\"  File: figures/functional_connectivity_matrix.pdf\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# FIGURE 2: 3D interactive connectome\n",
    "print(\"\\nGenerating Figure 2: 3D Interactive Connectome...\")\n",
    "\n",
    "try:\n",
    "    # Download atlas for coordinates\n",
    "    atlas = fetch_atlas_destrieux_2009(lateralized=True)\n",
    "    atlas_img = atlas['maps']\n",
    "    \n",
    "    # Load connectivity matrix with threshold\n",
    "    connectivity_matrix_thresholded = connectivity_matrix.copy()\n",
    "    threshold_85 = np.percentile(np.abs(connectivity_matrix_thresholded[np.nonzero(connectivity_matrix_thresholded)]), 85)\n",
    "    connectivity_matrix_thresholded[np.abs(connectivity_matrix_thresholded) < threshold_85] = 0\n",
    "    \n",
    "    # Create 3D interactive visualization\n",
    "    view = plotting.view_connectome(connectivity_matrix_thresholded, \n",
    "                                   atlas_img,\n",
    "                                   edge_threshold='85%')\n",
    "    \n",
    "    # Save\n",
    "    html_path = figures_dir / 'connectome_3d.html'\n",
    "    view.save_as_html(str(html_path))\n",
    "    \n",
    "    print(f\"✓ 3D interactive connectome saved: {html_path}\")\n",
    "except Exception as e:\n",
    "    print(f\"✗ Error creating 3D connectome: {e}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# FIGURE 3: Static connectome on brain (top 10% and 20%)\n",
    "print(\"\\nGenerating Figure 3: Static Connectome on Brain...\")\n",
    "\n",
    "try:\n",
    "    atlas = fetch_atlas_destrieux_2009(lateralized=True)\n",
    "    atlas_img = atlas['maps']\n",
    "    \n",
    "    fig, axes = plt.subplots(1, 2, figsize=(14, 6))\n",
    "    fig.suptitle('Static Connectome Visualization at Different Thresholds', \n",
    "                fontsize=10, fontweight='bold')\n",
    "    \n",
    "    # Top 10% connections\n",
    "    conn_10 = connectivity_matrix.copy()\n",
    "    threshold_10 = np.percentile(np.abs(conn_10[np.nonzero(conn_10)]), 90)\n",
    "    conn_10[np.abs(conn_10) < threshold_10] = 0\n",
    "    \n",
    "    plotting.plot_connectome(conn_10, atlas_img, \n",
    "                            display_mode='ortho',\n",
    "                            edge_threshold='90%',\n",
    "                            ax=axes[0],\n",
    "                            title='Top 10% Connections')\n",
    "    \n",
    "    # Top 20% connections\n",
    "    conn_20 = connectivity_matrix.copy()\n",
    "    threshold_20 = np.percentile(np.abs(conn_20[np.nonzero(conn_20)]), 80)\n",
    "    conn_20[np.abs(conn_20) < threshold_20] = 0\n",
    "    \n",
    "    plotting.plot_connectome(conn_20, atlas_img,\n",
    "                            display_mode='ortho',\n",
    "                            edge_threshold='80%',\n",
    "                            ax=axes[1],\n",
    "                            title='Top 20% Connections')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    \n",
    "    # Save\n",
    "    pdf_path = figures_dir / 'connectome_brain.pdf'\n",
    "    png_path = figures_dir / 'connectome_brain.png'\n",
    "    \n",
    "    plt.savefig(pdf_path, dpi=300, bbox_inches='tight')\n",
    "    plt.savefig(png_path, dpi=150, bbox_inches='tight')\n",
    "    plt.close()\n",
    "    \n",
    "    print(f\"✓ Static connectome visualization saved\")\n",
    "    print(f\"  PDF: {pdf_path}\")\n",
    "    print(f\"  PNG: {png_path}\")\nexcept Exception as e:\n",
    "    print(f\"✗ Error creating static connectome: {e}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# FIGURE 4: Hub regions bar chart\n",
    "print(\"\\nGenerating Figure 4: Hub Regions Bar Chart...\")\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(10, 8))\n",
    "\n",
    "# Top 15 hubs\n",
    "top_15 = metrics_df.head(15).copy()\n",
    "top_15 = top_15.sort_values('Betweenness_Centrality', ascending=True)\n",
    "\n",
    "# Get community colors\n",
    "community_colors = []\n",
    "for node_idx in top_15['Node'].values:\n",
    "    community_id = node_to_community.get(int(node_idx), 0)\n",
    "    community_colors.append(community_id)\n",
    "\n",
    "# Create color map\n",
    "n_communities = len(communities_data['communities'])\n",
    "cmap = plt.cm.get_cmap('tab20', n_communities)\n",
    "colors = [cmap(c) for c in community_colors]\n",
    "\n",
    "# Plot\n",
    "bars = ax.barh(range(len(top_15)), top_15['Betweenness_Centrality'].values, color=colors)\n",
    "\n",
    "ax.set_yticks(range(len(top_15)))\n",
    "ax.set_yticklabels(top_15['Region'].values, fontsize=8)\n",
    "ax.set_xlabel('Betweenness Centrality', fontsize=9)\n",
    "ax.set_title('Top 15 Hub Regions in Brain Connectome\\n(Colored by Community Membership)', \n",
    "            fontsize=10, fontweight='bold')\n",
    "ax.grid(True, alpha=0.3, axis='x')\n",
    "\n",
    "plt.tight_layout()\n",
    "\n",
    "# Save\n",
    "pdf_path = figures_dir / 'hub_regions.pdf'\n",
    "png_path = figures_dir / 'hub_regions.png'\n",
    "\n",
    "plt.savefig(pdf_path, dpi=300, bbox_inches='tight')\n",
    "plt.savefig(png_path, dpi=150, bbox_inches='tight')\n",
    "plt.close()\n",
    "\n",
    "print(f\"✓ Hub regions bar chart saved\")\n",
    "print(f\"  PDF: {pdf_path}\")\n",
    "print(f\"  PNG: {png_path}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# FIGURE 5: Network graph with spring layout\n",
    "print(\"\\nGenerating Figure 5: Network Graph with Spring Layout...\")\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(12, 12))\n",
    "\n",
    "# Use spring layout for positioning\n",
    "pos = nx.spring_layout(G, k=0.5, iterations=50, seed=42, weight='weight')\n",
    "\n",
    "# Node sizes proportional to betweenness centrality\n",
    "node_sizes = [metrics_df[metrics_df['Node'] == n]['Betweenness_Centrality'].values[0] * 5000 + 100\n",
    "              for n in G.nodes()]\n",
    "\n",
    "# Node colors by community\n",
    "n_communities = len(communities_data['communities'])\n",
    "cmap = plt.cm.get_cmap('tab20', n_communities)\n",
    "node_colors = [cmap(node_to_community.get(n, 0)) for n in G.nodes()]\n",
    "\n",
    "# Draw network\n",
    "# Draw edges with transparency proportional to weight\n",
    "edges = G.edges()\n",
    "weights = [G[u][v]['weight'] for u, v in edges]\n",
    "max_weight = max(weights)\n",
    "alphas = [w / max_weight * 0.5 + 0.1 for w in weights]\n",
    "\n",
    "for (u, v), alpha in zip(edges, alphas):\n",
    "    ax.plot([pos[u][0], pos[v][0]], [pos[u][1], pos[v][1]], \n",
    "           'k-', alpha=alpha, linewidth=0.5)\n",
    "\n",
    "# Draw nodes\n",
    "scatter = ax.scatter([pos[n][0] for n in G.nodes()],\n",
    "                     [pos[n][1] for n in G.nodes()],\n",
    "                     s=node_sizes,\n",
    "                     c=node_colors,\n",
    "                     alpha=0.8,\n",
    "                     edgecolors='black',\n",
    "                     linewidths=0.5)\n",
    "\n",
    "# Label top 10 hub nodes\n",
    "top_10_hubs = metrics_df.head(10)['Node'].values\n",
    "for node in top_10_hubs:\n",
    "    if node in pos:\n",
    "        ax.text(pos[node][0], pos[node][1], str(node), \n",
    "               fontsize=6, ha='center', va='center', fontweight='bold')\n",
    "\n",
    "ax.set_title('Brain Connectome Network Graph\\n(Node size = betweenness centrality, Color = community)', \n",
    "            fontsize=10, fontweight='bold')\n",
    "ax.axis('off')\n",
    "\n",
    "plt.tight_layout()\n",
    "\n",
    "# Save\n",
    "pdf_path = figures_dir / 'network_graph.pdf'\n",
    "png_path = figures_dir / 'network_graph.png'\n",
    "\n",
    "plt.savefig(pdf_path, dpi=300, bbox_inches='tight')\n",
    "plt.savefig(png_path, dpi=150, bbox_inches='tight')\n",
    "plt.close()\n",
    "\n",
    "print(f\"✓ Network graph saved\")\n",
    "print(f\"  PDF: {pdf_path}\")\n",
    "print(f\"  PNG: {png_path}\")"
   ]
  },
  {
   "cell_type": {"metadata": {},\n",
   "execution_count": null,\n",
   "outputs": [],\n",
    "source": [
    "# FIGURE 6: Connectivity heatmaps (placeholder for structural comparison)\n",
    "print(\"\\nGenerating Figure 6: Connectivity Heatmap Comparison...\")\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 6))\n",
    "fig.suptitle('Functional Connectivity Analysis', fontsize=10, fontweight='bold')\n",
    "\n",
    "# Full connectivity matrix\n",
    "sns.heatmap(connectivity_matrix, cmap='RdBu_r', center=0, vmin=-1, vmax=1,\n",
    "           square=True, cbar_kws={'label': 'Correlation'},\n",
    "           ax=axes[0], xticklabels=False, yticklabels=False)\n",
    "axes[0].set_title('Full Functional Connectivity Matrix', fontsize=9)\n",
    "\n",
    "# Thresholded connectivity matrix\n",
    "thresholded_conn = connectivity_matrix.copy()\n",
    "threshold = 0.3\n",
    "thresholded_conn[np.abs(thresholded_conn) < threshold] = 0\n",
    "\n",
    "sns.heatmap(thresholded_conn, cmap='RdBu_r', center=0, vmin=-1, vmax=1,\n",
    "           square=True, cbar_kws={'label': 'Correlation'},\n",
    "           ax=axes[1], xticklabels=False, yticklabels=False)\n",
    "axes[1].set_title(f'Thresholded Matrix (r > {threshold})', fontsize=9)\n",
    "\n",
    "plt.tight_layout()\n",
    "\n",
    "# Save\n",
    "pdf_path = figures_dir / 'struct_vs_func.pdf'\n",
    "png_path = figures_dir / 'struct_vs_func.png'\n",
    "\n",
    "plt.savefig(pdf_path, dpi=300, bbox_inches='tight')\n",
    "plt.savefig(png_path, dpi=150, bbox_inches='tight')\n",
    "plt.close()\n",
    "\n",
    "print(f\"✓ Connectivity comparison saved\")\n",
    "print(f\"  PDF: {pdf_path}\")\n",
    "print(f\"  PNG: {png_path}\")"
   ]
  },
