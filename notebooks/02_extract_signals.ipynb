 "cells": [
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Notebook 02: Extract Brain Signals\n",
    "\n",
    "Extract time series signals from brain regions using the Destrieux atlas."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Import all required libraries\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "import json\n",
    "from nilearn.input_data import NiftiLabelsMasker\n",
    "from nilearn import image\n",
    "from tqdm import tqdm\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "print(\"✓ All required libraries imported successfully\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Set random seed for reproducibility\n",
    "np.random.seed(42)\n",
    "\n",
    "print(\"✓ Random seed set to 42\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define paths using config\n",
    "notebook_dir = Path.cwd()\n",
    "project_dir = notebook_dir.parent\n",
    "data_dir = project_dir / 'data'\n",
    "figures_dir = project_dir / 'figures'\n",
    "\n",
    "# Load config\n",
    "config_path = data_dir / 'config.json'\n",
    "with open(config_path, 'r') as f:\n",
    "    config = json.load(f)\n",
    "\n",
    "atlas_path = config['atlas_path']\n",
    "func_files = config['func_files']\n",
    "confound_files = config['confound_files']\n",
    "tr = config['tr']\n",
    "n_subjects = config['n_subjects']\n",
    "\n",
    "print(f\"✓ Configuration loaded\")\n",
    "print(f\"  Atlas: {atlas_path}\")\n",
    "print(f\"  Number of subjects: {n_subjects}\")\n",
    "print(f\"  TR: {tr} seconds\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize NiftiLabelsMasker with Destrieux atlas\n",
    "print(\"Initializing NiftiLabelsMasker...\")\n",
    "masker = NiftiLabelsMasker(\n",
    "    labels_img=atlas_path,\n",
    "    standardize=True,  # Standardize time series\n",
    "    t_r=tr,  # Repetition time\n",
    "    verbose=0\n",
    ")\n",
    "\n",
    "print(\"✓ NiftiLabelsMasker initialized\")\n",
    "print(f\"  Standardize: True\")\n",
    "print(f\"  TR: {tr} seconds\")"
   ]
  },
  {
   "cell_type": name=\"Extract\",
   "metadata": {},\n",
   "outputs": [],
   "source": [
    "# Extract time series for all subjects\n",
    "print(f\"\\nExtracting time series for {n_subjects} subjects...\")\n",
    "all_time_series = []\n",
    "\n",
    "for i, func_file in enumerate(tqdm(func_files, desc=\"Extracting signals\")):\n",
    "    try:\n",
    "        # Extract time series\n",
    "        time_series = masker.fit_transform(func_file)\n",
    "        all_time_series.append(time_series)\n",
    "        \n",
    "        if (i + 1) % 5 == 0:\n",
    "            print(f\"  Subject {i+1}/{n_subjects}: shape {time_series.shape}\")\n",
    "    except Exception as e:\n",
    "        print(f\"  ✗ Error processing subject {i+1}: {e}\")\n",
    "\n",
    "print(f\"✓ Time series extracted for {len(all_time_series)} subjects\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Print shape of each subject's time series\n",
    "print(f\"\\nTime Series Shapes:\")\n",
    "for i, ts in enumerate(all_time_series[:5]):\n",
    "    print(f\"  Subject {i+1}: {ts.shape} (timepoints × regions)\")\n",
    "\n",
    "if len(all_time_series) > 5:\n",
    "    print(f\"  ... ({len(all_time_series) - 5} more subjects)\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Save all time series to data/all_time_series.npy\n",
    "timeseries_path = data_dir / 'all_time_series.npy'\n",
    "\n",
    "# Create a list to save (subjects may have different lengths)\n",
    "all_time_series_array = np.array(all_time_series, dtype=object)\n",
    "np.save(timeseries_path, all_time_series_array)\n",
    "\n",
    "print(f\"✓ All time series saved to {timeseries_path}\")\n",
    "print(f\"  File size: {timeseries_path.stat().st_size / (1024**2):.2f} MB\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Set matplotlib style for publication-quality figures\n",
    "plt.rcParams['font.family'] = 'DejaVu Sans'\n",
    "plt.rcParams['font.size'] = 8\n",
    "plt.rcParams['savefig.dpi'] = 300\n",
    "plt.rcParams['savefig.bbox'] = 'tight'\n",
    "\n",
    "print(\"✓ Matplotlib rcParams configured\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Plot time series of first subject for first 5 regions\n",
    "print(\"\\nGenerating time series sample plot...\")\n",
    "\n",
    "fig, axes = plt.subplots(5, 1, figsize=(12, 8))\n",
    "fig.suptitle('Sample Time Series: First 5 Brain Regions (Subject 1)', fontsize=10, fontweight='bold')\n",
    "\n",
    "first_subject_ts = all_time_series[0]\n",
    "n_timepoints_to_plot = min(500, first_subject_ts.shape[0])  # Plot first 500 timepoints\n",
    "\n",
    "for region_idx in range(5):\n",
    "    axes[region_idx].plot(first_subject_ts[:n_timepoints_to_plot, region_idx], linewidth=0.8, color='steelblue')\n",
    "    axes[region_idx].set_ylabel(f'Region {region_idx + 1}', fontsize=8)\n",
    "    axes[region_idx].grid(True, alpha=0.3)\n",
    "    axes[region_idx].set_xlim(0, n_timepoints_to_plot)\n",
    "\n",
    "axes[-1].set_xlabel('Timepoint', fontsize=8)\n",
    "plt.tight_layout()\n",
    "\n",
    "# Save as PDF and PNG\n",
    "pdf_path = figures_dir / 'time_series_sample.pdf'\n",
    "png_path = figures_dir / 'time_series_sample.png'\n",
    "\n",
    "plt.savefig(pdf_path, dpi=300, bbox_inches='tight')\n",
    "plt.savefig(png_path, dpi=150, bbox_inches='tight')\n",
    "plt.close()\n",
    "\n",
    "print(f\"✓ Time series plot saved:\")\n",
    "print(f\"  PDF: {pdf_path}\")\n",
    "print(f\"  PNG: {png_path}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Print completion summary\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"SIGNAL EXTRACTION SUMMARY\")\n",
    "print(\"=\"*60)\n",
    "print(f\"\\nSubjects processed: {len(all_time_series)}\")\n",
    "print(f\"Total timepoints per subject: {all_time_series[0].shape[0]}\")\n",
    "print(f\"Brain regions: {all_time_series[0].shape[1]}\")\n",
    "print(f\"\\nData saved:\")\n",
    "print(f\"  Time series: {timeseries_path}\")\n",
    "print(f\"  Sample plot: {pdf_path}\")\n",
    "print(f\"\\nNext Step: Run Notebook 03 (Functional Connectivity)\")\n",
    "print(\"=\"*60)"
   ]
  }
